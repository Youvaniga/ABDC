<Window x:Class="ABDC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ABDC"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="600">
    <Grid>
        <StackPanel VerticalAlignment="Top">
            <TextBlock Name="lblStatus" FontSize="22" FontWeight="Bold" Margin="20">ADBC</TextBlock>
            <Button Name="btnNUBEAccount" Click="btnNUBEAccount_Click" Width="150" Height="50" Margin="50" >NUBE Account</Button>
            <Button Name="btnFMCG" Click="btnFMCG_Click" Width="150" Height="50" Margin="50" >FMCG</Button>
        </StackPanel>
        
    </Grid>
</Window>
